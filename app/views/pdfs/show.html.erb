<p style="color: green"><%= notice %></p>

<%= render @pdf %>

<div>
  <ul style="list-style-type: none">
    <li style="float: left"><button class="btn btn-success"><%= link_to 'Read this pdf', read_pdf_pdf_path(@pdf), class: 'whitten' %></button>&nbsp</li>
    <li style="float: left"><button class="btn btn-warning"><%= link_to "Edit pdf", edit_pdf_path(@pdf) %></button> &nbsp </li>
    <li style="float: left"><button class="btn btn-info"><%= link_to "Home", pdfs_path %></button> &nbsp </li>
    <li style="float: left"><%= button_to "Destroy pdf", @pdf, method: :delete, class: 'btn btn-danger ' %> &nbsp </li>
  </ul>
</div>
